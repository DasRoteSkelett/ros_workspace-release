From ea11fdbf017f6e2f2bfbc44d564669dd567522d1 Mon Sep 17 00:00:00 2001
From: Jacob Perron <jacob@openrobotics.org>
Date: Tue, 21 Apr 2020 16:18:12 -0700
Subject: [PATCH 3/3] Drop Python 3.7 site packages from PYTHONPATH

Signed-off-by: Jacob Perron <jacob@openrobotics.org>
---
 debian/rules.em | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/debian/rules.em b/debian/rules.em
index 3c4251a..0adf63a 100755
--- a/debian/rules.em
+++ b/debian/rules.em
@@ -22,7 +22,7 @@ export DEB_CXXFLAGS_MAINT_APPEND=-DNDEBUG
 export DEB_HOST_MULTIARCH := $(shell dpkg-architecture -qDEB_HOST_MULTIARCH)
 
 # Needed to bootstrap since the ros_workspace package does not yet exist.
-export PYTHONPATH=@(InstallationPrefix)/lib/python3.8/site-packages:@(InstallationPrefix)/lib/python3.7/site-packages
+export PYTHONPATH=@(InstallationPrefix)/lib/python3.8/site-packages
 
 %:
 	dh $@@ -v --buildsystem=cmake
